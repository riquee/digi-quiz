{
   "name": "dig-backend",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "MIT",
   "scripts": {
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\"",
      "start": "node build/main.js",
      "start:prod": "node dist/main",
      "dev": "nest start --watch",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "migration:create": "env-cmd -f ../.env ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js -f ./src/config/orm.ts migration:create -n",
      "migration:run": "env-cmd -f ../.env ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js -f ./src/config/orm.ts migration:run",
      "migration:revert": "env-cmd -f ../.env ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js -f ./src/config/orm.ts migration:revert",
      "seed:create": "env-cmd -f ../.env ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js -f ./src/config/orm-seed.ts migration:create -n",
      "seed:run": "env-cmd -f ../.env ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js -f ./src/config/orm-seed migration:run",
      "seed:revert": "env-cmd -f ../.env ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js -f ./src/config/orm-seed.ts migration:revert"
   },
   "dependencies": {
      "@nestjs/common": "^8.0.0",
      "@nestjs/config": "^2.0.0",
      "@nestjs/core": "^8.0.0",
      "@nestjs/jwt": "^8.0.1",
      "@nestjs/mapped-types": "*",
      "@nestjs/passport": "^8.2.1",
      "@nestjs/platform-express": "^8.0.0",
      "@nestjs/typeorm": "^8.0.3",
      "bcrypt": "^5.0.1",
      "env-cmd": "^10.1.0",
      "passport": "^0.6.0",
      "passport-jwt": "^4.0.0",
      "passport-local": "^1.0.0",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "rxjs": "^7.2.0",
      "typeorm": "^0.2.17"
   },
   "devDependencies": {
      "@nestjs/cli": "^8.0.0",
      "@nestjs/schematics": "^8.0.0",
      "@nestjs/testing": "^8.0.0",
      "@types/express": "^4.17.13",
      "@types/node": "^16.11.10",
      "@types/passport-jwt": "^3.0.6",
      "@types/passport-local": "^1.0.34",
      "@typescript-eslint/eslint-plugin": "^5.0.0",
      "@typescript-eslint/parser": "^5.0.0",
      "eslint": "^8.0.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^4.0.0",
      "prettier": "^2.3.2",
      "source-map-support": "^0.5.20",
      "ts-loader": "^9.2.3",
      "ts-node": "10.7.0",
      "tsconfig-paths": "^3.10.1",
      "typescript": "4.5.2"
   }
}
